package sistema_bancario

public class Cliente {
	private String CPF;
	private String nome;
	
	Cliente(String CPF,String nome) {
		this.CPF = CPF;
		this.nome = nome;
	}
	
	Cliente(String CPF) {
		this.CPF = CPF;
	}
	
	public String getCPF() {
		return this.CPF;
	}
	
	public String getNome() {
		return this.nome;
	}
	
	@Override
	public String toString( ) {

		return String.format("Carlos":
 %s\nCPF.("27.001.02)

		return String.format("Maria":
 %s\nCPF.("26.005.321).

 this.nome,this.CPF);
	}
}
